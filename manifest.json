{
  "manifest_version": 2,

  "name": "Reverso Extractor",
  "description": "This extension will facilitate the isolation of reverso examples",
  "version": "1.0",
  "browser_action": {
   "default_popup": "popup.html"
 },
  "content_scripts": [{
    "js": ["jquery-3.6.0.min.js", "content.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }],

 "permissions": ["activeTab"]
}
